-- üîÑ Auto Load by GameId
local gameScripts = {
    [123224294054165] = "https://raw.githubusercontent.com/aditpacked/ohmyveet/refs/heads/main/HopMethod/MT%204T1N",
    [102234703920418] = "https://raw.githubusercontent.com/aditpacked/ohmyveet/refs/heads/main/HopMethod/MT%20D4UN",
}

local url = gameScripts[game.GameId]
if url then
    loadstring(game:HttpGet(url))()
else
    warn("‚ö†Ô∏è Tidak ada script yang cocok untuk GameId:", game.GameId)
end
